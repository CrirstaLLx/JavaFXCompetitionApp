<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="850.0" prefWidth="699.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.skgottalent.controllers.CompetitionController">
   <children>
       <Label fx:id="competitionLabel" />
      <Label fx:id="label" layoutX="311.0" layoutY="41.0">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <TableView fx:id="participantTableView" layoutX="55.0" layoutY="247.0" prefHeight="251.0" prefWidth="595.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="153.0" text="Name" />
          <TableColumn fx:id="ageColumn" prefWidth="134.0" text="Age" />
            <TableColumn fx:id="talentTypeColumn" prefWidth="159.0" text="Talent" />
            <TableColumn fx:id="scoreColumn" prefWidth="148.0" text="Score" />
        </columns>
      </TableView>
      <ListView fx:id="nextRoundListView" layoutX="53.0" layoutY="553.0" prefHeight="184.0" prefWidth="595.0" />
      <Label layoutX="237.0" layoutY="511.0" text="Participants on the next round">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button layoutX="275.0" layoutY="779.0" mnemonicParsing="false" onAction="#continueNextStage" prefHeight="35.0" prefWidth="149.0" text="Continue">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <TextArea fx:id="textArea" editable="false" layoutX="55.0" layoutY="90.0" prefHeight="142.0" prefWidth="595.0" />
   </children>
</AnchorPane>
